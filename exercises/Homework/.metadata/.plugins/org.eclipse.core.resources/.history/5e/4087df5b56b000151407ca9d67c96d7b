package testing;

public class test1 {
	
	public static void main(String[] args){
		int mostFrequent = 0;
		int count1= 0; int count2 = 0;
		int max1 = 0;
		int max2= 0;
		int Freq1 = 0; 
		int secFreq = 0;
		int Freq2 = 0;
		int[] myArray= {1, 1 ,3, 3, 4, 4, 4};
		for (int i = 0; i< myArray.length-2; i++){
			if (myArray[i] == myArray[i+1]){
				count1++;
				if (count1>max1){
					max1 = count1;
					Freq1 = myArray[i];
					count1 = 0;
				}
			}
			else if((myArray[i]!=myArray[i+1]) && (myArray[i+1] == myArray[i+2])){
				count2++;
					if (count2>max2){
						max2 = count2;
						Freq2 = myArray[i+1];
						count2 = 0;

					}
				}
				if(Freq1>Freq2){
					mostFrequent= Freq1;
					secFreq = Freq2;
				}
				else{
					secFreq = Freq1;
					mostFrequent = Freq2; 
				}
				
			}
				
		
		}
		
		
	}

