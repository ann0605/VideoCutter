package testing;

public class test3 {
	public static void main(String[] args){
		int[] myArray = {1,1,1,3,3,4,4,4,4,4};
		for (int i = 0; i< myArray.length; i++){
			while(myArray[i] == myArray[i+1]){
				count++;
			}
			if(count>max1){
				max1= count;
				count=0;
			}
			else if(count>max2 && count<max1){
				max2= count;
				count = 0;
			}
			else if(count<max2&&count<max1){
				max3 = count;
				count = 0;
			}
		}
		
	}
}
